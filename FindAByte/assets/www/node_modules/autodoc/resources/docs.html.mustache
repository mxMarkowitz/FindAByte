<!DOCTYPE html>
<html>

<head>
    <title>{{name}}</title>

    <link rel="stylesheet" type="text/css" href="docs.css" />
    <link rel="stylesheet" type="text/css" href="examples.css" />
    <link rel="stylesheet" type="text/css" href="highlight.css" />
    <script src="//pivotal.github.io/jasmine/lib/jasmine-1.3.1/jasmine.js"></script>
    <script src="//lodash.com/_js/lodash.js"></script>
    <script src="//benchmarkjs.com/_js/benchmark.js"></script>
    <script src="//code.jquery.com/jquery-1.10.1.min.js"></script>
    <script src="//code.highcharts.com/highcharts.js"></script>
    <script src="//danieltao.com/HighTables/hightables.js"></script>
    <script type="text/javascript" src="utils.js"></script>

    {{#javascripts}}
    <script type="text/{{{language}}}" src="{{{src}}}"></script>
    {{/javascripts}}

    <script type="text/javascript" src="defaults.js"></script>

    <script type="text/javascript">
      var benchmarks = {};
    </script>
</head>

<body>
    <main>
        <header>
            <h1>{{name}}</h1>

            {{#description}}
            {{{description}}}
            {{/description}}
        </header>

        <!-- Notification area for failing specs -->
        <div id="spec-failures"></div>

        <div class="search">
            <fieldset>
                <legend>Search</legend>
                <input type="text" name="search" placeholder="Start typing the name of a function or type" />
            </fieldset>
        </div>

        <div class="layout">
            <div class="index">
                <nav>
                    <h2>Functions</h2>
                    <ul>
                        {{#namespaces}}
                        {{^excludeFromDocs}}
                        <li class="nav-header">
                            {{#constructorMethod}}
                            <a href="#{{identifier}}">{{name}}</a>
                            {{/constructorMethod}}
                            {{^constructorMethod}}
                            {{namespace}}
                            {{/constructorMethod}}
                        </li>
                        {{#members}}
                        {{^excludeFromDocs}}
                        <li><a href="#{{identifier}}" class="{{descriptor}}">{{shortName}}</a></li>
                        {{/excludeFromDocs}}
                        {{/members}}
                        {{/excludeFromDocs}}
                        {{/namespaces}}
                    </ul>

                    {{#hasTypes}}
                    <h2>Types</h2>
                    <ul>
                        {{#types}}
                        {{^excludeFromDocs}}
                        <li><a href="#{{identifier}}">{{name}}</a></li>
                        {{/excludeFromDocs}}
                        {{/types}}
                    </ul>
                    {{/hasTypes}}
                </nav>
            </div>

            <article>
                {{#namespaces}}
                {{#allMembers}}
                {{^excludeFromDocs}}
                <section class="{{sectionType}}" id="{{identifier}}" data-name="{{lowerCaseName}}" data-acronym="{{acronym}}" data-filter="{{searchName}}">
                    <h1>
                        {{shortName}}
                        <small>{{name}}</small>
                    </h1>

                    {{{description}}}

                    {{#hasSignature}}
                    <div class="signature">
                        <h3>Signature</h3>

                        <pre>{{{highlightedSignature}}}</pre>

                        <pre class="function-source" id="source-{{identifier}}">{{{highlightedSource}}}</pre>

                        <table id="{{name}}-parameters">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Type(s)</th>
                                    <th>Description</th>
                                </tr>
                            </thead>

                            <tbody>
                                {{#params}}
                                <tr>
                                    <td>{{name}}</td>
                                    <td>{{type}}</td>
                                    <td>{{{description}}}</td>
                                </tr>
                                {{/params}}

                            </tbody>

                            {{^isConstructor}}
                            <tfoot>
                                {{#returns}}
                                <tr>
                                    <td>returns</td>
                                    <td>{{type}}</td>
                                    <td>{{{description}}}</td>
                                </tr>
                                {{/returns}}
                            </tfoot>
                            {{/isConstructor}}
                        </table>
                    </div>
                    {{/hasSignature}}

                    {{#hasExamples}}
                    <div class="examples">
                        <h3>Examples</h3>

                        {{#examples}}
                        <pre class="source-examples" id="examples-{{identifier}}">{{{highlightedCode}}}</pre>
                        {{/examples}}
                    </div>
                    {{/hasExamples}}

                    {{#hasBenchmarks}}
                    <div class="perf">
                        <h3>Benchmarks</h3>

                        {{#benchmarks}}
                        <pre class="cm-s-default">{{{highlightedCode}}}</pre>

                        <table id="{{identifier}}-perf-tests">
                            <tr>
                                <th>Implementation</th>
                                {{#cases}}
                                <th>{{label}}</th>
                                {{/cases}}
                            </tr>

                            {{#list}}
                            <tr id="perf-test-{{identifier}}-{{id}}">
                                <td>{{{name}}}</td>
                                {{#cases}}
                                <td data-case-id="{{caseId}}"></td>
                                {{/cases}}
                            </tr>
                            {{/list}}
                        </table>

                        <button>Run performance benchmarks</button>
                        {{/benchmarks}}
                    </div>
                    {{/hasBenchmarks}}

                    <script type="text/javascript">
                      window.addEventListener('load', function() {
                        {{#privateMembers}}
                        var {{shortName}} = {{{source}}};
                        {{#methods}}
                        {{longName}} = {{{source}}};
                        {{/methods}}
                        {{/privateMembers}}

                        {{{exampleHelpers}}}

                        {{> examples}}
                        {{> benchmarks}}
                      });
                    </script>
                </section>
                {{/excludeFromDocs}}
                {{/allMembers}}
                {{/namespaces}}

                {{#types}}
                {{^excludeFromDocs}}
                    {{> typeSection}}
                {{/excludeFromDocs}}
                {{/types}}
            </article>
        </div>
    </main>

    <script type="text/javascript" src="docs.js"></script>
</body>

</html>
