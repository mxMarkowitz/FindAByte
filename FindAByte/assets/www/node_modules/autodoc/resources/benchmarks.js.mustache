{{#hasBenchmarks}}
(function() {
  var functionName = '{{name}}';

  {{#benchmarks}}

  {{{setup}}}

  benchmarks['{{identifier}}'] = [];

  {{#list}}
  {{#cases}}
  (function() {
    var benchmark = new Benchmark('{{name}}', {
      id: '{{identifier}}-{{id}}-{{caseId}}',
      fn: function() {
        {{{impl}}};
      }
    });

    benchmark.functionName = functionName;
    benchmark.benchmarkId = '{{identifier}}-{{id}}';
    benchmark.caseId = {{caseId}};

    benchmarks['{{identifier}}'].push(benchmark);
  }());

  {{/cases}}
  {{/list}}
  {{/benchmarks}}
}());
{{/hasBenchmarks}}
